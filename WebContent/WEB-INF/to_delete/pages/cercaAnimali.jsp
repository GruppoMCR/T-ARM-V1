<form id="cercaAnimali" action="test">
  <div class="form-group">
    <label for="nomeAnimale">Nome</label>
    <input class="form-control" id="nomeAnimale" name="nomeAnimale" placeholder="Inserisci il nome del cane">
</div>
  <div class="form-group">
    <label for="razzaAnimale">Razza</label>
    <input class="form-control" id="razzaAnimale" name="razzaAnimale" placeholder="Inserisci la razza dell'animale">
  </div>
  <div class="form-group">
    <label for="selezionaSesso">Sesso</label>
    <select class="form-control" id="selezionaSesso" name="selezionaSesso">
      <option></option>
      <option>Maschio</option>
      <option>Femmina</option>
    </select>
  </div>
  <button type="submit" class="btn btn-primary">Cerca</button>
</form>
